{
    "manifest_version": 3,
    "name": "Test Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [{
      "matches": ["http://*/*"],
      "js": ["content.js"]
    }]
}
